package view;

import java.awt.BorderLayout;
import java.awt.EventQueue;

import javax.swing.JFrame;
import javax.swing.JPanel;
import javax.swing.border.EmptyBorder;
import javax.swing.JLabel;
import java.awt.Font;
import javax.swing.JScrollPane;
import javax.swing.JButton;

public class GroupMember extends JFrame {

	private JPanel contentPane;

	/**
	 * Launch the application.
	 */
	public static void main(String[] args) {
		GroupMember gp = new GroupMember();
	}

	/**
	 * Create the frame.
	 */
	public GroupMember() {
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setBounds(100, 100, 300, 300);
		contentPane = new JPanel();
		contentPane.setBorder(new EmptyBorder(5, 5, 5, 5));
		setContentPane(contentPane);
		contentPane.setLayout(null);
		
		JLabel lblGroupName = new JLabel("Group Name:");
		lblGroupName.setFont(new Font("Lucida Grande", Font.PLAIN, 15));
		lblGroupName.setBounds(32, 6, 100, 30);
		contentPane.add(lblGroupName);
		
		JLabel label = new JLabel("TBD");
		label.setFont(new Font("Lucida Grande", Font.PLAIN, 15));
		label.setBounds(158, 6, 100, 30);
		contentPane.add(label);
		
		JScrollPane scrollPane = new JScrollPane();
		scrollPane.setBounds(32, 48, 244, 189);
		contentPane.add(scrollPane);
		
		JPanel panel = new JPanel();
		scrollPane.setColumnHeaderView(panel);
		
		JButton btnOk = new JButton("OK");
		btnOk.setBounds(101, 243, 117, 29);
		contentPane.add(btnOk);
	}
}
